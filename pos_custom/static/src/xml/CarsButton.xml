<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
  <t t-name="CarsButton" owl="1">
      <div class="control-button" style="color:black" t-on-click="() => this.trigger('click-car')">
          <i class="fa fa-truck"> </i>
          <span> </span>
          <t t-if="currentOrderCar">
            <t t-esc="currentOrderCar" />
          </t>
          <t t-else="">
            Car
          </t>
      </div>
  </t>
</templates>
